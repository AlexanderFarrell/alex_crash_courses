<%
 var games = [
     {
         "name": "Ordered Energy",
         "url": "/r/ordered_energy",
         "icon": "OrderedEnergyLow.png",
         "desc": "Strategize and Succeed"
     },
     {
         "name": "You Cannot",
         "url": "/r/you_cannot",
         "icon": "ycbtglow.png",
         "desc": "Dive into Chaos"
     },
     {
         "name": "Stock Simulator",
         "url": "/r/stock_simulator",
         "icon": "StockSimulatorLow.png",
         "desc": "Explore the Economy"
     }
 ]
 var gamesDev = [
     {
         "name": "Buried Peace",
         "url": "/r/buried_peace",
         "icon": "dlow.png",
         "desc": ""
     },
     {
         "name": "Raise Triton",
         "url": "/r/raise_triton",
         "icon": "clow.png",
         "desc": ""
     },
     {
         "name": "Void Money",
         "url": "/r/void_money",
         "icon": "VoidMoney2low.png",
         "desc": ""
     }
 ]
 %>
<div class="mainMid">
    <div class="ContentItem MinHeight">
        <br>
        <h1>Games</h1>
        <div class="Games">
            <% for (var i = 0; i < games.length; i++) { %>
                <a href="<%= games[i].url%>">
                    <div class="turquoiseMid Game">
                        <img src="/images/marbles/low/<%= games[i].icon%>">
                        <div><%= games[i].name %></div>
<!--                        <div class="GameDesc"><%= games[i].desc %></div>-->
                    </div>
                </a>
            <% } %>
        </div>
        <br>
        <h2>In Development</h2>
        <div class="Games">
            <% for (var i = 0; i < gamesDev.length; i++) { %>
                <a href="<%= gamesDev[i].url%>">
                    <div class="turquoiseMid Game">
                        <img src="/images/marbles/low/<%= gamesDev[i].icon%>">
                        <div><%= gamesDev[i].name %></div>
<!--                        <div class="GameDesc"><%= gamesDev[i].desc %></div>-->
                    </div>
                </a>
            <% } %>
        </div>
        <br>
    </div>
</div>

